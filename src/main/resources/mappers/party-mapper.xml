<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Party">
<select resultType="int" id="getNextval">
select party_seq.nextval from dual 
</select>
<insert id="insert">
	insert into party values (#{seq},#{parent_name}, #{parent_address}, #{title}, #{writer}, #{meetdate}, #{count}, #{gender}, #{age}, #{drinking}, #{content},#{status})
</insert>

<select id="selectBySeq" resultType="coma.spring.dto.PartyDTO">
	select * from party where seq = #{value} order by seq desc
</select>

</mapper>